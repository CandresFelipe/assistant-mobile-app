{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"strict": true,
		"jsx": "react-native",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["app/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"extends": "expo/tsconfig.base"
}
